.header-icon[href*="patreon"] {
	
	&::before {
		.fontawesomeBrands;
		color: @black;
		content: "\f3d9";
		vertical-align: middle;
	}

	svg {
		display: none;
		visibility: hidden;
	}
}

.header-icon[href*="mastodon"] {
	height: 28px !important;
	
		&::before {
			.fontawesomeBrands;
			color: @black;
			content: "\f4f6";
			font-size: 24px;
			margin-top: 0.05em;
		}
		
		svg {
			display: none;
			visibility: hidden;
		}
}

a.header-icon {
	&[href*="bsky"] {
		height: 28px !important;
	
		&::before {
			.fontawesomeBrands;
			color: @black;
			content: "\e671";
			font-size: 24px;
			margin-top: 0.05em;
		}
		
		svg {
			display: none;
			visibility: hidden;
		}
	}
}
