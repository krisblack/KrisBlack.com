.sqs-block-socialaccountlinks-v2 .sqs-svg-icon--wrapper[href*="patreon"] {

&::before {
	.fontawesomeBrands;
	color: inherit;
	content: "\f3d9";
	display: block;
	font-size: 130%;
	height: 100%;
	width: 100%;
	margin-top: -0.15em;}
	
	svg {
		display: none;
		visibility: hidden;
	}
}

.sqs-block-socialaccountlinks-v2 .sqs-svg-icon--wrapper[href*="mastodon"] {

	&::before {
		.fontawesomeBrands;
		color: inherit;
		content: "\f4f6";
		display: block;
		font-size: 149%;
		height: 100%;
		width: 100%;
		margin-top: -0.15em;
	}
		
	svg {
		display: none;
		visibility: hidden;
	}
}

