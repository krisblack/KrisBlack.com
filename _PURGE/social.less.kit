//Embed Responsibly for Videos
.embed-container {
  position: relative;
  padding-bottom: 56.25%;
  height: 0;
  overflow: hidden;
  max-width: 100%;
}

.embed-container iframe, 
.embed-container object, 
.embed-container embed { 
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.social-icons-style-regular {
  .facebook-unauth .sqs-use--icon { fill: @facebookColor !important; }   .instagram-unauth .sqs-use--icon { fill: @instagramColor !important; }   .twitter-unauth .sqs-use--icon { fill: @twitterColor !important; }   .youtube-unauth .sqs-use--icon { fill: @youtubeColor !important; }
  
  .facebook-unauth:hover .sqs-use--icon { fill: lighten(@facebookColor, 10%) !important; }   .instagram-unauth:hover .sqs-use--icon { fill: lighten(@instagramColor, 10%) !important; }   .twitter-unauth:hover .sqs-use--icon { fill: lighten(@twitterColor, 10%) !important; }   .youtube-unauth:hover .sqs-use--icon { fill: lighten(@youtubeColor, 10%) !important; }
}

.social-icons-style-knockout {
  .facebook-unauth .sqs-use--icon { fill: white !important; }   .instagram-unauth .sqs-use--icon { fill: white !important; }   .twitter-unauth .sqs-use--icon { fill: white !important; }   .youtube-unauth .sqs-use--icon { fill: white !important; }
  
  .facebook-unauth:hover .sqs-use--icon { fill: white !important; }   .instagram-unauth:hover .sqs-use--icon { fill: white !important; }   .twitter-unauth:hover .sqs-use--icon { fill: white !important; }   .youtube-unauth:hover .sqs-use--icon { fill: white !important; }
  
  .facebook-unauth .sqs-use--mask { fill: @facebookColor !important; }   .instagram-unauth .sqs-use--mask { fill: @instagramColor !important; }   .twitter-unauth .sqs-use--mask { fill: @twitterColor !important; }   .youtube-unauth .sqs-use--mask { fill: @youtubeColor !important; }
  
  .facebook-unauth:hover .sqs-use--mask { fill: lighten(@facebookColor, 10%) !important; }   .instagram-unauth:hover .sqs-use--mask { fill: lighten(@instagramColor, 10%) !important; }   .twitter-unauth:hover .sqs-use--mask { fill: lighten(@twitterColor, 10%) !important; }   .youtube-unauth:hover .sqs-use--mask { fill: lighten(@youtubeColor, 10%) !important; }
}