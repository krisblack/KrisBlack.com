a.sqs-svg-icon--wrapper[href*="patreon"],
a.icon[href*="patreon"] {
  svg {
    display: none;
  }
  
  div {
    align-items: center;
    display: flex;
    justify-content: center;
    
    &:before {
      .fontawesomeBrands();
      color: white;
      content: "\f3d9";
      display: inline-block;
    }
  }
}

.social-icons-style-regular {
  a.sqs-svg-icon--wrapper[href*="patreon"],
  a.icon[href*="patreon"] {
    div {      
      &:before {
        color: @patreonColor;
      }
    }
  }
}

.social-icons-style-knockout {
  a.sqs-svg-icon--wrapper[href*="patreon"],
  a.icon[href*="patreon"] {
    div {
      background-color: @patreonColor;
      
      &:before {
        color: white;
      }
    }
  }
}

